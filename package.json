  "devDependencies": {
    "@11ty/eleventy": "1.0.*",
    "@11ty/eleventy-navigation": "0.3.*",
    "@fortawesome/fontawesome-free": "5.15.*",
    "a11y-color-tokens": "0.5.*",
    "gulp": "4.0.*",
    "gulp-concat": "2.6.*",
    "gulp-sass": "5.0.*",
    "include-media": "1.4.*",
    "npm-run-all": "4.1.*",
    "sass": "1.43.*"
  },
  "main": "gulpfile.js",
  "scripts": {
    "tokens": "npx a11y-color-tokens --colorTokensPath='./src/_data/colors.js' --outputDirPath='./src/_sass/color' --compatibilityDocsPath='./src/guide'",
    "build:sass": "gulp",
    "build:eleventy": "npx eleventy",
    "build": "npm-run-all tokens build:sass build:eleventy"
  }
